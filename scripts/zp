#!/bin/bash

#==================================================================
#211_zone_ip 기능을 중지한다.
#==================================================================

#==================================================================
# 211_config.sh 설정 파일 읽어오기 
_CONFIG_FILE="$(dirname "$(readlink -f "$0")")/211_config.sh"
if [ -f "$_CONFIG_FILE" ] ; then
    source "$_CONFIG_FILE"
else
    echo "[$_CONFIG_FILE] FILE NOT EXIST"
    exit 1
fi
#==================================================================

touch "$_ZONE_IP_STOP_FILE"

